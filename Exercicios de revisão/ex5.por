programa {
  funcao inicio() {
    real altura, largura, provundidade, volume

    escreva("Infome a altura da caixa de água")
    leia(altura)

    escreva("Informe a largura da caixa de água")
    leia(largura)

    escreva("Informe a provundidade da caixa de água")
    leia(provundidade)

    volume = altura*largura*provundidade

    escreva("O volume é de", volume)
    
  }
}
